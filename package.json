{
  "name": "react-native-bbs-signatures",
  "version": "0.12.0",
  "description": "ffi-bbs-signatures wrapper for react native",
  "react-native": "lib/index.js",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "files": [
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native.config.js",
    "react-native-bbs-signatures.podspec",
    "README.md"
  ],
  "binary": {
    "module_name": "libs",
    "host": "https://github.com/animo/ffi-bbs-signatures/releases/download/",
    "remote_path": "{version}",
    "package_name": "android.zip",
    "module_path": "."
  },
  "scripts": {
    "check-types": "tsc --noEmit -p tsconfig.build.json",
    "example": "yarn --cwd example",
    "prettier": "prettier --ignore-path .gitignore '**/*.+(js|json|ts|md|yml|yaml)'",
    "format": "yarn prettier --write",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./lib",
    "compile": "tsc -p tsconfig.build.json",
    "tscodegen": "react-native-tscodegen ./react-native-tscodegen.json"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/animo/react-native-bbs-signatures.git",
  "author": "Animo Solutions <berend@animo.id> (https://animo.id)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/animo/react-native-bbs-signatures.git/issues"
  },
  "homepage": "https://github.com/animo/react-native-bbs-signatures.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "react-native-tscodegen": "0.68.4",
    "react-native-tscodegen-types": "0.67.3"
  },
  "devDependencies": {
    "@mathquis/node-pre-gyp-github": "^1.0.2",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.66.0",
    "prettier": "^2.0.5",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
